		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
		>

	<bean class="org.geomajas.plugin.staticsecurity.configuration.SecurityServiceInfo">
		<property name="users">
			<list>
                <bean class="org.geomajas.plugin.staticsecurity.configuration.UserInfo">
                    <property name="password" value="PwofgU2BD04NlWr6clUYYA" />
                    <property name="userName" value="Rest User" />
                    <property name="userId" value="admin" />
                    <property name="userOrganization" value="Geomajas" />
                    <property name="userLocale" value="nl_BE" />
                    <property name="userDivision" value="JSON rest services" />
                </bean>
			</list>
		</property>
	</bean>

</beans>
